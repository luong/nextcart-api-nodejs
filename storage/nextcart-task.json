{
  "containerDefinitions": [
    {
      "name": "nextcart-api",
      "image": "luongfox/nextcart-api-nodejs:develop",
      "cpu": 1024,
      "memory": 2048,
      "memoryReservation": 1024,
      "portMappings": [
        {
          "name": "nextcart-api-80-tcp",
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp",
          "appProtocol": "http"
        }
      ],
      "essential": true,
      "environment": [],
      "environmentFiles": [
        {
          "value": "arn:aws:s3:::nextcart/setting/dev.env",
          "type": "s3"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "workingDirectory": "/app",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/nextcart-task1",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        },
        "secretOptions": []
      },
      "healthCheck": {
        "command": [
          "CMD-SHELL",
          "curl -f http://127.0.0.1/products/1 || exit 1"
        ],
        "interval": 30,
        "timeout": 5,
        "retries": 3
      }
    }
  ],
  "family": "nextcart-task",
  "taskRoleArn": "arn:aws:iam::058156486034:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::058156486034:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "runtimePlatform": {
    "cpuArchitecture": "X86_64",
    "operatingSystemFamily": "LINUX"
  },
  "tags": []
}